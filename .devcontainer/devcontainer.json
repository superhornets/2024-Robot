  // https://github.com/wpilibsuite/allwpilib/releases/latest
  "postCreateCommand": "cd /opt && wget 'https://github.com/wpilibsuite/vscode-wpilib/releases/download/v2024.2.1/vscode-wpilib-2024.2.1.vsix'",
  "features": {
  },
  "customizations": {
    "vscode": {
